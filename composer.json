        {
            "name": "Зорин Алексей",
            "email": "zorinalexey59292@gmail.com"
        }
    ],
    "support": {
        "email": "zorinalexey59292@gmail.com",
        "source": "https://github.com/zorinalexey/cloud-castle-sms-services-laravel",
